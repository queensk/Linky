{
  "manifest_version": 3,
  "name": "Linky",
  "version": "1.0",
  "description": "A browser extension to bookmark and store links.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "popup.js"
  }
}
